{
  "id": "flash-ship",
  "protocol_version": "0.2.0",
  "identity": {
    "name": "번개 배송 주식회사",
    "purpose": "당일 배송 및 국제 물류 서비스를 제공합니다."
  },
  "offers": [
    {
      "intent": "calculate_shipping",
      "description": "무게와 거리에 따른 배송비를 계산합니다.",
      "endpoint": "/api/v1/calc",
      "method": "POST",
      "side_effect": "read-only",
      "auth_type": "none"
    },
    {
      "intent": "request_delivery",
      "description": "배송을 의뢰하고 송장을 발급합니다.",
      "endpoint": "/api/v1/ship",
      "method": "POST",
      "side_effect": "state-change",
      "auth_type": "api_key"
    }
  ]
}